use std::net::TcpListener;

struct Router {
    uint: u8
}

impl  Router {
    
    pub fn new() -> Self {
        let listener = TcpListener::bind("127.0.0.1:8080").unwrap();

        for stream in listener.incoming() {
            let stream = stream.unwrap();
        }
        let status = 0;
        Self {
            uint: status
        }
    }

    pub fn route(route: String) {
        
    }

}



